    "editor.tokenColorCustomizations": {
      "[Flatland Monokai]": {
        "textMateRules": [
          {
            "scope": [
              "storage.type.type.ts",
              "storage.type.class.ts"
            ],
            "settings": {
              "foreground": "#F92672"
            }
          },
          {
            "scope": "storage.type.interface.ts",
            "settings": {
              "foreground": "#F92672"
            }
          },
          {
            "scope": [
              "storage.type.ts",
              "storage.type.function.ts"
            ],
            "settings": {
              "foreground": "#F92672"
            }
          },
          {
            "scope": "meta.function-call.python meta.function-call.generic.python",
            "settings": {
              "foreground": "#a6e22e",
              "fontStyle": "italic",
            }
          },
          {
            "scope": "variable.language.special.self.python",
            "settings": {
              "foreground": "#fd971f",
              "fontStyle": "italic bold"
            }
          },
          {
            "scope": "variable.parameter",
            "settings": {
              "foreground": "#fd971f",
              "fontStyle": "italic",
            }
          },
          {
            "scope": [
              "source.python meta.class.python entity.name.type.class.python",
            ],
            "settings": {
              "foreground": "#66D9FF",
              "fontStyle": "italic bold",
            }
          },{
            "scope": [
              "variable.language.super.ts",
            ],
            "settings": {
              "foreground": "#66D9FF",
            }
          },
          {
            "scope": "meta.function.python entity.name.function.python",
            "settings": {
              "foreground": "#a6e22e",
              "fontStyle": "bold",
            }
          },
          {
            "scope": "entity.other.inherited-class",
            "settings": {
              "foreground": "#34d058",
              "fontStyle": "bold underline italic",
            }
          },
          {
            "scope": "variable.language.this.ts",
            "settings": {
              "foreground": "#FD971F",
            }
          },
          {
            "scope": "source.tsx meta.interface.tsx storage.type.interface.tsx",
            "settings": {
              "foreground": "#F92672",
            }
          },
          {
            "scope": "source.tsx meta.var.expr.tsx storage.type.tsx",
            "settings": {
              "foreground": "#F92672",
            }
          },
          {
            "scope": [
              "storage.type.type.tsx"
            ],
            "settings": {
              "foreground": "#F92672",
            }
          }
        ],
      },
      "[GitHub Light]": {
        "textMateRules": [
          {
            "scope": [
              "string",
              "punctuation.definition.string",
              "string punctuation.section.embedded source"
            ],
            "settings": {
              "foreground": "#003470",
            }
          },
          {
            "scope": [
              "source.vue text.html.derivative meta.tag.other.unrecognized.html.derivative entity.name.tag.html",
            ],
            "settings": {
              "foreground": "#005CC5",
            }
          },
          {
            "scope": [
              "meta.arrow.ts storage.type.function.arrow.ts",
            ],
            "settings": {
              "foreground": "#F97583"
            }
          },
          {
            "scope": [
              "support.class.component.tsx"
            ],
            "settings": {
              "foreground": "#BF3989"
            }
          }
        ]
      },
      "[GitHub Dark]": {
        "textMateRules": [
          {
            "scope": [
              "string",
              "punctuation.definition.string",
              "string punctuation.section.embedded source"
            ],
            "settings": {
              "foreground": "#fff08f",
            }
          },
          {
            "scope": [
              "source.vue text.html.derivative meta.tag.other.unrecognized.html.derivative entity.name.tag.html",
            ],
            "settings": {
              "foreground": "#79B8FF",
            }
          },
          {
            "scope": [
              "meta.arrow.ts storage.type.function.arrow.ts",
            ],
            "settings": {
              "foreground": "#F97583"
            }
          },
          {
            "scope": [
              "support.class.component.tsx"
            ],
            "settings": {
              "foreground": "#f183c5"
            }
          }
        ]
      },
    },
