      "[Andromeda]": {
        "enabled": true,
        "rules": {
          "selfParameter": {
            "foreground": "#FF00AA",
            "fontStyle": "italic"
          },
          "variable": {
            "foreground": "#fdeedf"
          },
          "property": {
            "foreground": "#fdeedf"
          },
          "class": {
            "foreground": "#66D9EF",
          },
          "module": {
            "foreground": "#fdeedf"
          }
        }
      },
      "[GitHub Light]": {
        "enabled": true,
        "rules": {
          "class": {
            "foreground": "#BF3989"
          },
          "interface": {
            "foreground": "#BF3989"
          },
          "type": {
            "foreground": "#BF3989"
          },
          "enum": {
            "foreground": "#BF3989"
          },
          "property": {
            "foreground": "#109c00",
          },
          "function": {
            "foreground": "#6f42c1",
          },
          "clsParameter": {
            "foreground": "#e36209",
            "fontStyle": "italic",
          },
          "selfParameter": {
            "foreground": "#e36209",
            "fontStyle": "italic",
          },
          "typeParameter": {
            "foreground": "#e36209"
          },
          "typeParameter.declaration": {
            "foreground": "#e36209",
          },
          "magicFunction": {
            "foreground": "#BF3989"
          },
          "razorTagHelperAttribute": {
            "foreground": "#ff283d"
          },
          "xmlDocCommentName": {
            "foreground": "#22863A"
          },
          "razorComponentElement": {
            "foreground": "#BF3989"
          },
        }
      },
      "[GitHub Dark]": {
        "enabled": true,
        "rules": {
          "class": {
            "foreground": "#f183c5"
          },
          "interface": {
            "foreground": "#f183c5"
          },
